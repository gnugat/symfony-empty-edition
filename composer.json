{
    "name": "gnugat/symfony-empty-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Empty Edition\" distribution",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "tests\\": "tests/" }
    },
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.1.8",
        "symfony/finder": "^7.1.6",
        "symfony/framework-bundle": "^7.1.6",
        "symfony/yaml": "^7.1.6"
    },
    "config": {
        "bump-after-update": true,
        "sort-packages": true
    }
}
