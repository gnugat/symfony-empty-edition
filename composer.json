{
    "name": "gnugat/symfony-empty-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Empty Edition\" distribution",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "tests\\": "tests/" }
    },
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "incenteev/composer-parameter-handler": "^2.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": [
            { "file": "app/config/parameters.yaml" }
        ]
    }
}
